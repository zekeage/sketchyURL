
<head>
<title>Enter a URL to make it sketchy:</title>
</head>
<body>

<textarea value= "" id = "originalurl" style = "width:500px;" ></textarea>
<br>
<button id="makeurl" onclick="makeurl(this)">MakeSketchyURL!</button>
<br>
<textarea value= "" id = "newurl" style = "width:500px;"></textarea>

<%= form_for :url do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

</body>
